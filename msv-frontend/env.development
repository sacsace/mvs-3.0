# MVS 3.0 Frontend - Development Environment

# API Configuration
REACT_APP_API_URL=https://localhost/api
REACT_APP_WS_URL=wss://localhost:5001

# App Configuration
REACT_APP_NAME=MVS 3.0
REACT_APP_VERSION=3.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_SENTRY=false

# External Services
REACT_APP_SENTRY_DSN=
REACT_APP_GOOGLE_ANALYTICS_ID=

# Development Tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_REACT_QUERY_DEVTOOLS=true

# API Timeout
REACT_APP_API_TIMEOUT=10000

# File Upload
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf

# Pagination
REACT_APP_DEFAULT_PAGE_SIZE=20
REACT_APP_MAX_PAGE_SIZE=100

# Cache Configuration
REACT_APP_CACHE_TTL=300000
REACT_APP_CACHE_MAX_SIZE=50

# Theme Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_DEFAULT_LANGUAGE=ko

# Security
REACT_APP_ENABLE_HTTPS=false
REACT_APP_ENABLE_CSP=true
